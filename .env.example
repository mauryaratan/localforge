# LocalForge Environment Variables
# Copy this file to .env.local and update values as needed

# Your production site URL (used for sitemap, robots.txt, and metadata)
# Defaults to https://localforge.app if not set
NEXT_PUBLIC_SITE_URL=https://localforge.app

# Enable Vercel Analytics (set to "true" to enable)
# Only works when deployed on Vercel
NEXT_PUBLIC_ENABLE_ANALYTICS=false
